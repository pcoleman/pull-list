{
  "manifest_version": 2,
  "name": "PullList",
  "description": "An extension to manage and track a digital comicbook pull list.",
  "version": "0.1",
  "background": {
    "persistent": false,
    "scripts": ["src/jquery/jquery.min.js","src/comicvine.js"]
  },
  "permissions": [ "https://www.comicvine.com/api/*" ],
  "browser_action": {
    "default_title": "PullList",
    "default_icon": {
	"19": "assets/icon_19.png",
	"38": "assets/icon_38.png"
    },
    "default_popup": "content/window.html"
  },
  "icons": { "19": "assets/icon_19.png", "38": "assets/icon_38.png", "128": "assets/icon_128.png" }
}
